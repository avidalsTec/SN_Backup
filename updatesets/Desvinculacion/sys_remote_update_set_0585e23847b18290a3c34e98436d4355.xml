gr.addQuery('parent_case', current.sys_id);
gr.query();
while (gr.next()) {
    if (gr.state != '3') {
        gs.addErrorMessage(gs.getMessage("The case cannot be solved because have open tasks"));
        action.openGlideRecord(current);
        current.setAbortAction(true);
    }
}

if (current.close_notes.nil()) {
    gs.addErrorMessage(gs.getMessage("Please specify solution in close notes"));
    action.openGlideRecord(current);
    current.setAbortAction(true);
